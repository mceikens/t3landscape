{
  "name": "mceikens/t3landscape",
  "description": "TYPO3 CMS webpage for TYPO3 ecosystem landscape",
  "license": "proprietary",
  "type": "project",
  "version": "1.0.0",
  "prefer-stable": true,
  "minimum-stability": "stable",
  "require": {
    "php": "^8.4",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-sodium": "*",
    "helhum/dotenv-connector": "^3.0",
    "helhum/typo3-console": "^8.2",
    "typo3/cms-adminpanel": "^14.1",
    "typo3/cms-backend": "^14.1",
    "typo3/cms-belog": "^14.1",
    "typo3/cms-beuser": "^14.1",
    "typo3/cms-core": "^14.1",
    "typo3/cms-dashboard": "^14.1",
    "typo3/cms-extbase": "^14.1",
    "typo3/cms-extensionmanager": "^14.1",
    "typo3/cms-felogin": "^14.1",
    "typo3/cms-filelist": "^14.1",
    "typo3/cms-filemetadata": "^14.1",
    "typo3/cms-fluid": "^14.1",
    "typo3/cms-fluid-styled-content": "^14.1",
    "typo3/cms-form": "^14.1",
    "typo3/cms-frontend": "^14.1",
    "typo3/cms-info": "^14.1",
    "typo3/cms-install": "^14.1",
    "typo3/cms-lowlevel": "^14.1",
    "typo3/cms-reactions": "^14.1",
    "typo3/cms-redirects": "^14.1",
    "typo3/cms-reports": "^14.1",
    "typo3/cms-rte-ckeditor": "^14.1",
    "typo3/cms-scheduler": "^14.1",
    "typo3/cms-seo": "^14.1",
    "typo3/cms-setup": "^14.1",
    "typo3/cms-tstemplate": "^14.1",
    "mceikens/logwriter": "^1.0",
    "mceikens/sitepackage": "^1.0"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.4",
    "typo3/coding-standards": "^0.8.0",
    "typo3/testing-framework": "^9.3",
    "deployer/deployer": "^7.5",
    "phpspec/prophecy": "*",
    "phpspec/prophecy-phpunit": "*",
    "jangregor/phpstan-prophecy": "*",
    "php-mock/php-mock-phpunit": "*",
    "phpmd/phpmd": "^2.15",
    "mockery/mockery": "^1.6",
    "phpstan/phpstan-mockery": "^1.1"
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/*"
    }
  ],
  "config": {
    "allow-plugins": {
      "helhum/dotenv-connector": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "helhum/dotenv-connector": {
      "env-file": ".env",
      "adapter": "Helhum\\DotEnvConnector\\Adapter\\SymfonyDotEnv"
    },
    "helhum/typo3-connector": {
      "install-extension-dummy": false
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public"
    }
  },
  "authors": [
    {
      "name": "Eike Drost",
      "email": "eike.drost@mceikens.de",
      "homepage": "https://www.mceikens.de/",
      "role": "Maintainer"
    }
  ]
}
